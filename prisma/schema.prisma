generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider             = "mysql"
  url                  = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

model data {
  id    String  @id @default(cuid())
  type  String?
  value Json?
}

model strnData {
  id           String  @id @default(cuid())
  collectionId String?
  documentId   String?
  key          String?
  type         String?
  value        String?
}

model strnDocument {
  id  String  @id @default(cuid())
  key String?
}

model strnCollection {
  id  String  @id @default(cuid())
  key String?
}
